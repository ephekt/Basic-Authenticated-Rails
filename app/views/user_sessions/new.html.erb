<h1>Login</h1>
 
<% form_for @user_session, :url => user_session_path do |f| %>
  <%= f.error_messages %>
  <%= render :partial => "users/form", :object => f %>
  <%= f.check_box :remember_me %><%= f.label :remember_me %><br />
  <br />
  <%= f.submit "Login" %> or <%= link_to "Forgot password", new_password_reset_path %>
<% end %>